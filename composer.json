{
    "name": "cesargb/laravel-magiclink",
    "description": "Create link for authenticate in Laravel without password",
    "keywords": [
        "laravel",
        "auth",
        "link"
    ],
    "homepage": "https://github.com/cesargb/laravel-magiclink",
    "authors": [
	{
		"name": "Cesar Garcia",
		"email": "cesargb@gmail.com",
		"homepage": "https://github.com/cesargb",
        "role": "Developer"
	}
    ],
    "license": "MIT",
    "require": {
        "php" : "^7.1",
        "illuminate/auth": "^5.5|~6.0.0",
        "illuminate/container": "^5.5|~6.0.0",
        "illuminate/contracts": "^5.5|~6.0.0",
        "illuminate/database": "^5.5|~6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cesargb\\MagicLink\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesargb\\MagicLink\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan --no-progress src tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesargb\\MagicLink\\MagicLinkServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.2|^3.8.0|^4.0",
        "phpunit/phpunit": "^7.0|^8.0",
        "phpstan/phpstan": "^0.11.8"
    }
}
